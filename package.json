{
    "name": "web-skeleton",
    "version": "1.0.0",
    "description": "A basic skeleton for creating websites with a Sass-TypeScript toolchain.",
    "main": "index.js",
    "scripts": {
        "lint-scss": "npx stylelint \"**/*.scss\"",
