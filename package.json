        "css-loader": "^6.8.1",
        "eslint": "^8.49.0",
        "html-webpack-plugin": "^5.5.3",
        "mini-css-extract-plugin": "^2.7.6",
        "style-loader": "^3.3.3",
        "stylelint": "^15.10.3",
        "stylelint-config-standard-scss": "^11.0.0",
        "ts-loader": "^9.5.0",
        "typescript": "^5.2.2",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "dependencies": {
        "@types/react": "^18.2.34",
